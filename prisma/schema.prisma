generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int     @id @default(autoincrement())
  name         String  @db.VarChar(45)
  email        String  @unique @db.VarChar(45)
  password     String  @db.VarChar(500)
  refreshToken String? @db.VarChar(500)
}
